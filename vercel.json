{
  "rewrites": [
    {
      "source": "/application",
      "destination": "/src/pages/application/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    }
  ],
  "middleware": true
}
