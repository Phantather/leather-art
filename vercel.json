{
  "version": 2,
  "routes": [
    {
      "src": "/.*",
      "headers": {
        "Accept-Ranges": "none",
        "Cache-Control": "public, max-age=3600, must-revalidate",
        "Content-Type": "text/html; charset=utf-8"
      },
      "continue": true
    }
  ],
  "rewrites": [
    {
      "source": "/application",
      "destination": "/src/pages/application/index.html"
    }
  ]
}
