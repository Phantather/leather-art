{
  "rewrites": [
    {
      "source": "/application",
      "destination": "/src/pages/application/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Accept-Ranges",
          "value": "none"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600, must-revalidate"
        },
        {
          "key": "Content-Type",
          "value": "text/html; charset=utf-8"
        },
        {
          "key": "Vary",
          "value": "Accept-Encoding"
        }
      ]
    }
  ],
  "routes": [
    {
      "src": "/.*",
      "headers": {
        "Accept-Ranges": "none"
      },
      "continue": true
    }
  ]
}
